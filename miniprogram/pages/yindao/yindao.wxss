}

.textcontent {
  color: white;
  margin: auto;

}

.inputbox {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  margin-top: -160rpx;
  border-bottom: 1px solid #f1f1f1;
