  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.man {
  margin-top: 15rpx;
  width: 360rpx;
  height: 360rpx;
  margin-left: 13rpx;
}

.woman {
  margin-top: 10rpx;
  width: 360rpx;
  height: 360rpx;
}

.years {
  margin-top: 50rpx;
