.yindao_name {
  width: 90%;
  height: 100rpx;
  margin-top: 80rpx;
  margin-left: 5%;
  margin-right: 5%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 40rpx;

}

.yindao_tishi {
  width: 90%;
  height: 100rpx;
  margin-top: 10rpx;
  margin-left: 5%;
  margin-right: 5%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
}

.sex {
  width: 90%;
  height: 420rpx;
  margin-top: 10rpx;
  margin-left: 5%;
  margin-right: 5%;
  display: flex;
